<div class='text-center'>  
  <div class='deities-pointer'>
    <%= link_to image_tag("Deities.png"), deities_path %>
  </div>
  <div class='mythologies-pointer'>
    <%= link_to "Mythologies", mythologies_path %>
  </div>
  <div class='myths-pointer'>
    <%= link_to "Myths", events_path %>
  </div>
  <div class='user-logging-pointer'>
  <% if @user %>
      <%= link_to "Logout", logout_path, method: 'delete', :controller=> "sessions", :action=> "destroy" %>
  <% else %>
      <%= link_to "Login", login_path %>
  <% end %>
  </div>
  <div class='api-docs-pointer'>
    <%= link_to "API Documentation", api_dashboard_path %>
  </div>
</div>
